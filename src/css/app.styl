// app global css
body 
    font-family: 'Cabin', sans-serif!important
.q-layout-page
    padding:30px
.q-toolbar-title 
    -webkit-box-flex: 1;
    -ms-flex: 1 1 0%;
    flex: 1 1 0%;
    min-width: 1px;
    max-width: 100%;
    font-size: 28px;
    font-weight: 500;
    padding: 10px 12px;
.q-item-sublabel
    color: #757575;
    font-size: 90%;
    margin-top: .2rem;
.q-toolbar a
    color:#fff
.q-item-icon
    color: var(--q-color-secondary)
.q-layout-page
    text-shadow: 5px 5px 12px #47242429
    max-width:1000px
h1 
    font-size:4em;
    line-height:1em;
    margin-bottom:10px;
    display:block;
    text-align:center;
    color:#004346;
    font-family: 'Source Code Pro', sans-serif!important
h2
    background: #0043463b;
    font-size:1.4em;
    display:block;
    text-align:center;
    margin-top:-35px;
    padding:15px;
    padding-top:35px;
    font-family: 'Source Code Pro', sans-serif!important
    border-radius: 10px;